    context = "tei:*[ starts-with( @spanTo, '#') ]"
    "\x{a}" ~
    "         "
    sch:assert [
      test = "id( substring( @spanTo, 2 ) ) >> ."
      "\x{a}" ~
      "	      The element indicated by @spanTo ("
      sch:value-of [ select = "@spanTo" ]
      ") must follow the current "
      sch:name [ ]
      " element \x{a}" ~
      "            "
    ]
    "\x{a}" ~
    "      "
  ]
  "\x{a}" ~
  "   "
]
